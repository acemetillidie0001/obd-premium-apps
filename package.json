{
  "name": "cursor-app-build",
  "version": "0.1.1",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "predev": "powershell -ExecutionPolicy Bypass -File scripts/predev.ps1",
    "dev": "next dev",
    "dev:reset": "powershell -ExecutionPolicy Bypass -File scripts/dev-reset.ps1",
    "fix:turbopack": "powershell -ExecutionPolicy Bypass -File scripts/fix-turbopack-corruption.ps1",
    "validate:build-no-db": "node tools/validate-build-no-db.cjs",
    "build": "next build",
    "build:prod": "pnpm run build",
    "build:vercel": "node scripts/vercel-build.mjs",
    "vercel-build": "pnpm run build:vercel",
    "start": "next start",
    "lint": "eslint src/app/api/social-auto-poster src/app/apps/social-auto-poster src/lib/apps/social-auto-poster src/lib/premium.ts src/lib/auth.ts src/app/api/auth/[...nextauth] --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern",
    "lint:repo": "eslint src --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern",
    "lint:ci": "eslint src/app/apps/obd-scheduler/page.tsx src/components/obd/OBDToast.tsx src/lib/dev/assertNever.ts --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern",
    "typecheck": "pnpm -s prisma:generate && tsc --noEmit",
    "check": "pnpm -s lint:ci && pnpm -s typecheck",
    "ci": "pnpm -s validate:build-no-db && pnpm -s check && pnpm -s vercel-build",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "playwright test tests/e2e/a11y.spec.ts",
    "test:visual": "playwright test tests/e2e/scheduler.spec.ts --grep \"Visual Regression\"",
    "test:load": "k6 run tests/load/k6-scheduler.js",
    "test:all": "pnpm test && pnpm test:e2e && pnpm test:a11y",
    "postinstall": "prisma generate || echo 'Warning: prisma generate failed (DATABASE_URL may be missing). This is OK for local dev without DB connection.'",
    "migrate:deploy": "tsx scripts/validate-db-url.ts && prisma generate && prisma migrate deploy && prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "deploy:migrate": "pnpm run migrate:deploy",
    "db:env:check": "node -r dotenv/config -e \"console.log('HAS_DATABASE_URL=',!!process.env.DATABASE_URL); try{console.log('DB_HOST=',new URL(process.env.DATABASE_URL).hostname)}catch(e){console.log('DB_HOST=parse-fail')}\"",
    "db:resolve:all": "node tools/resolve-all-failed-migrations.cjs",
    "db:resolve-failed-migrations": "node tools/resolve-all-failed-migrations.cjs",
    "migrate:resolve": "tsx scripts/resolve-failed-migration.ts",
    "verify:db": "tsx scripts/verify-db.ts",
    "check:deploy": "tsx scripts/check-deployment-status.ts",
    "db:generate": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "db:sync": "prisma generate && prisma migrate deploy",
    "db:status": "prisma migrate status",
    "db:studio": "prisma studio",
    "db:exec": "prisma db execute --file",
    "db:resolve:review": "prisma migrate resolve --rolled-back 20251225045724_add_review_request_automation_tables",
    "db:check": "node tools/check-migrations.cjs",
    "db:inspect": "node tools/db-inspect.cjs",
    "db:inspect:ts": "tsx tools/db-inspect.ts",
    "check:business-integrity": "tsx scripts/check-business-integrity.ts",
    "business:create": "tsx scripts/create-business.ts",
    "dev:bootstrap-business": "tsx scripts/dev-bootstrap-business.ts",
    "memberships:backfill": "tsx scripts/backfill-business-memberships.ts",
    "memberships:verify": "node --import tsx scripts/verify-memberships.ts",
    "memberships:grant": "node --import tsx scripts/grant-membership.ts",
    "crm:db:fix": "powershell -ExecutionPolicy Bypass -File scripts/obd-crm-db-fix.ps1"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.2.0",
    "@vercel/blob": "^2.0.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "install": "^0.13.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.561.0",
    "next": "^16.0.10",
    "next-auth": "^5.0.0-beta.30",
    "nodemailer": "^7.0.11",
    "open": "^11.0.0",
    "openai": "^6.10.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "resend": "^6.6.0",
    "server-only": "^0.0.1",
    "stripe": "^20.3.0",
    "twilio": "^5.11.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.16",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.8",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "typescript-eslint": "8.50.1",
    "vitest": "^4.0.16"
  }
}
